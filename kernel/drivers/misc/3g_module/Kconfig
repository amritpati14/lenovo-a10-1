#
# 3G device configuration
#

menuconfig 3G_MODULE
	tristate "3G module for phonepad"
	---help---
	  Say Y here if you have a support modem

if 3G_MODULE
    config MU509
		bool "MU509"

    config MT6229
    	bool "MT6229"

    config MW100
    	bool "MW100"

    config SEW868
		bool "SEW868"	

    config MI700
		bool "MI700"	
	config SC6610
		bool "SC6610"	
endif
